  "scripts": {
    "android": "react-native run-android",
    "ios": "npx run react-native run-ios --hostname 0.0.0.0",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "beep": "afplay /System/Library/Sounds/Glass.aiff",
    "android4": "adb -s RZ8R122YPAP reverse tcp:9500 tcp:9500 && NODE_OPTIONS='--max-http-header-size=16384' npx react-native run-android --device=R7AX50TT9XV",
    "android5": "npx react-native run-android --device=R7AX50TT9XV",
    "android5-fast": "cd android && ./gradlew assembleDebug -x lint -x test && adb -s R7AX50TT9XV install -r app/build/outputs/apk/debug/app-debug.apk && adb -s R7AX50TT9XV shell am start -n com.idlockupdater/.MainActivity",
    "android5-build": "cd android && ./gradlew assembleDebug -x lint -x test",
    "android5-install": "adb -s R7AX50TT9XV install -r android/app/build/outputs/apk/debug/app-debug.apk && adb -s R7AX50TT9XV shell am start -n com.idlockupdater/.MainActivity",
    "pods": "cd ios && pod install && cd ..",
    "rebuild": "rm -rf node_modules && rm -rf node_modules && npm i && npm run pods && npm run beep",
    "a-rebuild": "npm run a-clean && npm run android5 && npm run beep",
    "a-clean": "rm -rf android/app/build/generated/autolinking android/app/.cxx android/app/build/generated android/build/generated && cd android && ./gradlew clean",
    "postinstall": ""
  },
